<div id="viewport">
	<div id="ui">
		<div class="close control"></div>
		<select name="biketype" id="biketype">
			<option><%=t :normal_bike %></option>
			<option disabled><%=t :frieght_bike %></option>
			<option disabled><%=t :electrical_bike %></option>
		</select>
		<div class="reset"><%=t :reset %></div>
		<div class="label">
			<label>
				<span><%=t :from %></span>
				<input type="text" class="from"  title='<%=t :from %>'>
			</label>
		</div>
		<div class="label">
			<label>
				<span><%=t :to %></span>
				<input type="text" class="to"  title='<%=t :to %>'>
			</label>
		</div>




		<div class="time">
			<p class='departure'><%=t :departure %> <input type="text" /></p>
			<p class='arrival'><%=t :arrival %>  <input type="text" /></p>
			<p class='distance'><%=t :distance %> <span class="count">23 km</span></p>
			<p class='duration'><%=t :duration %> <span class="count">13 minutter</span></p>
		</div>

		<div class="meta">
			<a class="link" href="#"><%=t :link_for_route %></a> 
			<p class="instructions"><%=t :route_instructions %></p>
			<div class="route">
				
			</div>
		</div>

	</div>
	<div id="map"></div>
</div>

